# hostname of the switch (read from the system)
switch fooswitch

#switch-nvapi-host switchHost
#switch-nvapi-login switchLogin
#switch-nvapi-password switchPassword

# List of collectors
collector fooCollector1 {
	collector-ip-address 192.168.0.1
	collector-hostname 192.168.0.1
	transport udp
	transport-port 9991
	dscp 0
}

collector fooCollector2 {
	collector-ip-address 192.168.0.2
	collector-hostname 192.168.0.2
	transport udp
	transport-port 9992
	dscp 0
}
